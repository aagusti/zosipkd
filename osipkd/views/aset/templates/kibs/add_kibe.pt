<html metal:use-macro="load: kibs.pt">
<div metal:fill-slot="content">
      <div class="form-group"> 
        <div tal:define="field form['e_judul']" id="item-${field.oid}">
          <label for="${field.oid}" class="control-label col-md-1" id="req-${field.oid}">
                  ${field.title}</label>
          <div class="col-md-5">
            ${structure:field.serialize()}
              <p id="error-${field.oid}" class="help-block" tal:condition="field.error"
               tal:repeat="error field.error.messages()">
               ${error}</p>
          </div>
        </div>
        <div tal:define="field form['e_jenis']" id="item-${field.oid}">
