		<script>
		var a, b, c, jml1;
		a = $('#vol_1_1').val();
		b = $('#vol_1_2').val();
		c = $('#hsat_1').val();
		jml1 = (a*b)*c;
		$('#jml1').val(formatNumber(jml1));
		$('#hsat_1').val(formatNumber(c));
		
        function formatNumber(n) {
            return n.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".");
