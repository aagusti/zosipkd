  <script type='text/javascript'>
      var smod = "/b103/002"
      var mID;
      var oTable;
      var iPos;
      var oTableUrl = smod+"/grid2?spp_id="+${rows and rows.id or 0}; 
      var tb_array = [
          '<div class="btn-group pull-left">',
          ' <button id="btn_tambah" class="btn btn btn-primary" type="button">Tambah</button>',
          ' <button id="btn_hapus" class="btn btn btn-danger" type="button">Hapus</button>',
          ' &nbsp;',
          '</div>'
      ];

      var tb = tb_array.join(' ');

      $(document).ready(function () {
        oTable = $('#table1').dataTable({
            "sAjaxSource"    : oTableUrl,
            "bServerSide"    : true,
            "bProcessing"    : true,
            /*"sDom": '<"toolbar">lfrtip',*/
            "dom"            : '<"top"ilf>rt<"bottom"<"toolbar">p><"clear">',
            "bScrollCollapse": true,
            "bSort"          : true,
            "bInfo"          : false,
            "bFilter"        : true,
            "bAutoWidth"     : false,
            "bPaginate"      : true,
            "sPaginationType": "full_numbers",
            "bUseRendered"   : true,
            
            "lengthMenu": [
                [10, 25, 50, -1],
                [10, 25, 50, "All"]
            ],
            "aoColumnDefs": [{
                "bSearchable": false, "bVisible": false,"aTargets": [0]
                },{
                   "sClass"   : "right",
                   "aTargets" : [6],
                   "mRender"  : function (data, type, full) {
                   return formatNumber(data);}
                },
                {
                  "render": function ( data, type, row ) {
                     if (row[2]=="1") return "UP";
                     else if (row[2]=="2") return "TU";
                     else if (row[2]=="3") return "GU";
                     else if (row[2]=="4") return "LS";
                     else return "";
                   //return data +' ('+ row[3]+')';
                     },
                        "targets": 2
                }
            ],

            "aoColumns": [
                null, 
                {"sWidth": "25px"},   
                {"sWidth": "25px"}, 
                {"sWidth": "25px"},
                {"sWidth": "100px"},
                {"sWidth": "100px"},
                {"sWidth": "100px"},
 
            ],

        });

        function formatNumber(n) {
                 return n.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".");
                 }

        $("div.toolbar").html(tb);

        $('#table1 tbody').on('click', 'tr', function () {
            if ($(this).hasClass('selected')) {
                mID = ''; ;
                $(this).removeClass('selected');
            } else {
                iPos1 = oTable.fnGetPosition(this);
                var aData = oTable.fnGetData(iPos1);
                mID = aData[0];
                oTable.$('tr.selected').removeClass('selected');
                $(this).addClass('selected');
            }
        });
        $('#btn_hapus').click(function () {
          if (mID) {
              var hapus = confirm('Hapus data ini?');
              if (hapus == true) {
                  $.ajax({
                      type: "POST",
                      url: smod+"/delitem",
                      data: "id=" + mID,
                      success: function (html) {
                          if (html.success == true) {
                              $("#errors").css('display', 'inline', 'important');
                              $("#errors").html("<span class='glyphicon glyphicon-ok'></span> " + html.msg);
                              var oTable = $('#table1').dataTable();
                              oTable.fnReloadAjax();
                          } else {
                              $("#errors").css('display', 'inline', 'important');
                              $("#errors").html("<span class='glyphicon glyphicon-remove'></span> " + html.msg);
                          }
                      },
                      beforeSend: function () {
                          $("#errors").css('display', 'inline', 'important');
                          $("#errors").html("<img src=\'${request.static_url('osipkd:static/images/ajax-loader.gif')}\' /> Loading...")
                      }
                  });
                  return false;
              };
          } else {
              alert('Silahkan pilih data yang akan dihapus');
          }
        });
              
        /*FORM --------------------------*/
                
        $("#btn_save").click(function () {
            $.ajax({
                type: "POST",
                url: smod+"/save",
                data: $("#myform").serialize(),
                success: function (html) {
                    if (html.success == true) {
                        $("#errors").css('display', 'inline', 'important');
                        $("#errors").html("<span class='glyphicon glyphicon-ok'></span> " + html.msg);
                    } else {
                        $("#errors").css('display', 'inline', 'important');
                        $("#errors").html("<span class='glyphicon glyphicon-remove'></span> " + html.msg);
                    }
                },
                beforeSend: function () {
                    $("#errors").css('display', 'inline', 'important');
                    $("#errors").html("<img src=\'${request.static_url('osipkd:static/images/ajax-loader.gif')}\' /> Loading...")
                }
            });
            return false;
        });

        $("#btn_close").click(function () {
            window.location = "/b103/002";
            return false;
        });	

		$( "#tanggal,#ap_tgl_kontrak" ).datepicker({dateFormat:'dd-mm-yy'});

      });
        </script>
  
