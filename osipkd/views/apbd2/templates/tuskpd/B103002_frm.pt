        <script> 
                var sppID, sppKd, sppNi, sppNm; 
                var iPos1;
                var tblspp;
                var tblsppUrl = "/apbd/dlg/spd?pk_id="+$('#unit_id').val();
                 
            $(document).ready(function () {
                $('#btn_spd').click(function() {
                    
                    /* Kondisi dimana data belum disimpan 
                      if ($('#id').val()=="" || $('#id').val()=="0"){
                      alert('Simpan Dulu Master Data');
                      return;
                    }*/
                     
                    var tblsppUrl = "/apbd/dlg/spd?pk_id="+$('#unit_id').val();
                    tblspp.fnReloadAjax(tblsppUrl);
                    $('#dlgSpp').modal('show');
                });
            
                tblspp = $('#tblspp').dataTable({
                    "sAjaxSource"    : tblsppUrl,
                    "bServerSide"    : true,
                    "bProcessing"    : true,
                    "sDom"           : '<"toolbar">lfrtip',
                    "bScrollCollapse": true,
                    "bSort"          : true,
                    "bInfo"          : false,
                    "bFilter"        : true,
                    "bAutoWidth"     : false,
                    "bPaginate"      : true,
                    "sPaginationType": "full_numbers",
                    
