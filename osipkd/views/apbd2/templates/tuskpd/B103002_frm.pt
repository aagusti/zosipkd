                var ttdID1, barangID2, barangKd, barangNi, barangNm; 
                var iPos1;
                var tblbarang;
                var tblbarangUrl = "/apbd/dlg/barang?pk_id="+$('#unit_id').val();
                 
            $(document).ready(function () {
                $('#btn_barang').click(function() {
                    var tblbarangUrl = "/apbd/dlg/barang?pk_id="+$('#unit_id').val();
                    tblbarang.fnReloadAjax(tblbarangUrl);
                    $('#dlgBarang').modal('show');
                });
            
                tblbarang = $('#tblbarang').dataTable({
                    "sAjaxSource"    : tblbarangUrl,
                    "bServerSide"    : true,
                    "bProcessing"    : true,
                    "sDom"           : '<"toolbar">lfrtip',
                    "bScrollCollapse": true,
                    "bSort"          : true,
                    "bInfo"          : false,
                    "bFilter"        : true,
                    "bAutoWidth"     : false,
                    "bPaginate"      : true,
                    "sPaginationType": "full_numbers",
                    
                    "lengthMenu": [
                        [10, 25, 50, -1],
                        [10, 25, 50, "All"]
                    ],
                    "aoColumnDefs": [
                    {"bSearchable": false, "bVisible": false, "aTargets": [0,1] }, 
                    {"bSearchable": false, "bVisible": true,  "aTargets": [2,3,4] },
                    ],

                    "aoColumns": [
                        null, 
                        {"sWidth": "15%", "sClass": "left"},
                        {"sWidth": "25%", "sClass": "left"},
                        {"sWidth": "25%", "sClass": "left"}
                    ],
                });
                
                $('#tblbarang tbody').on('click', 'tr', function () {
                    if ($(this).hasClass('selected')) {
                       barangID1 = ''; barangID2 = ''; barangKd=''; barangNi=''; barangNm=''; 
                        $(this).removeClass('selected');
                    } else {
                        iPos1 = tblbarang.fnGetPosition(this);
                        var aData = tblbarang.fnGetData(iPos1);
                        barangID1 = aData[0];
                        barangID2 = aData[1];
                        barangKd  = aData[2];
                        barangNi  = aData[3];
                        barangNm  = aData[4];
                        tblbarang.$('tr.selected').removeClass('selected');
                        $(this).addClass('selected');
                    }
                });
                
             
                $('#btn_pil_barang').click(function() {
                    if(barangID1) {
                       
                        $('#dlgBarang').modal('hide');
                        $('#barang_uid').val(barangID2);
                        $('#barang_nip').val(barangKd);
                        $('#barang_nama').val(barangNi);
                        $('#barang_jab').val(barangNm);
                        
                    } else
                        alert ('Silahkan pilih salah satu atau batal!');
                });
           }); 
           /*end of Barang*/     
        </script>
        <!-- Modal barang -->
        <div class="modal fade" id="dlgBarang" tabindex="-1" role="dialog" data-backdrop="static">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title" id="dlgBarangLabel">Pilih Barang</h4>
                    </div>
                    <div class="modal-body">
                        <table class="table table-bordered table-hover table-condensed" id="tblbarang">
                            <thead>
                                <tr>
                                   <th>Index</th>
                                   <th>Index</th>
                                   <th>NIP</th>
                                   <th>Nama</th>
                                   <th>Jabatan</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>        
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Batal</button>
                        <button type="button" class="btn btn-primary" id="btn_pil_barang" name="btn_pil_barang">Pilih</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Modal -->
  </div>
</html>